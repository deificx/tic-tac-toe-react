{
  "extends": "tslint:latest",
  "rules": {
    "arrow-parens": false,
    "member-access": [true, "no-public"],
    "no-reference": false,
    "ordered-imports": false,
    "quotemark": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  }
}
