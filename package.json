{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://deificx.github.io/tic-tac-toe-react",
  "scripts": {
    "build": "react-scripts-ts build",
    "css": "tcm src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts-ts eject",
    "start": "react-scripts-ts start",
    "test": "react-scripts-ts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.14",
    "@types/ramda": "^0.27.20",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "gh-pages": "^3.1.0",
    "react-scripts-ts": "4.0.8",
    "typed-css-modules": "^0.6.4",
    "typescript": "^4.0.3"
  }
