{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["dom", "es6"],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "es5",
    "esModuleInterop": true,
    "paths": {"react": ["node_modules/@types/react"]}
  }
}
